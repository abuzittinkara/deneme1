{
  "name": "test",
  "version": "1.0.0",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/src/app.js",
    "start:render": "node dist/render-app.js",
    "start:full": "node dist/src/app.js",
    "dev": "nodemon --exec ts-node --transpile-only src/app.ts",
    "dev:js": "nodemon src/app.js",
    "build": "ts-node --transpile-only scripts/build.ts && npm run copy:locales",
    "build:src": "tsc --project tsconfig.json --outDir ./dist --noEmit false --skipLibCheck true --allowJs true --checkJs false && npm run copy:locales",
    "build:scripts": "tsc --project tsconfig.scripts.json --outDir ./dist/scripts --noEmit false --skipLibCheck true",
    "build:client": "cd public && npm run build",
    "build:frontend": "ts-node scripts/buildFrontend.ts",
    "build:all": "npm run build:src && npm run build:client && npm run build:frontend",
    "postinstall": "echo 'Run npm run build to compile TypeScript'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "typecheck": "tsc --noEmit --skipLibCheck true",
    "clean": "rimraf dist",
    "clean:client": "rimraf public/dist",
    "clean:all": "npm run clean && npm run clean:client",
    "clean:js": "ts-node scripts/cleanup.ts",
    "check": "npm run lint && npm run typecheck",
    "fix": "npm run lint:fix && npm run format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:handlers": "jest --testPathPattern=tests/integration/socket",
    "test:public": "cd public && npm test",
    "test:all": "npm test && npm run test:public",
    "migrate:js-to-ts": "ts-node scripts/migrateJsToTs.ts",
    "ts:convert": "ts-node scripts/convertToTypeScript.ts",
    "ts:clean": "ts-node scripts/cleanJsFiles.ts",
    "ts:check": "tsc --noEmit --skipLibCheck true",
    "ts:migrate": "node scripts/ts-migration.js",
    "ts:strict": "tsc --noEmit --strict",
    "ts:strict-check": "node scripts/ts-strict-check.js",
    "ts:strict-check:all": "node scripts/ts-strict-check.js --flag=all",
    "ts:strict-check:noImplicitAny": "node scripts/ts-strict-check.js --flag=noImplicitAny",
    "ts:strict-check:strictNullChecks": "node scripts/ts-strict-check.js --flag=strictNullChecks",
    "ts:strict-check:strictFunctionTypes": "node scripts/ts-strict-check.js --flag=strictFunctionTypes",
    "ts:strict-check:strictPropertyInitialization": "node scripts/ts-strict-check.js --flag=strictPropertyInitialization",
    "ts:strict-check:noImplicitThis": "node scripts/ts-strict-check.js --flag=noImplicitThis",
    "security:scan": "ts-node scripts/security-scan.ts",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "copy:locales": "powershell -Command \"if (!(Test-Path dist/locales)) { New-Item -ItemType Directory -Path dist/locales -Force }; if (!(Test-Path dist/src/locales)) { New-Item -ItemType Directory -Path dist/src/locales -Force }; Copy-Item -Path src/locales/*.json -Destination dist/locales/ -Force; Copy-Item -Path src/locales/*.json -Destination dist/src/locales/ -Force\""
  },
  "dependencies": {
    "@sentry/integrations": "^7.44.1",
    "@sentry/node": "^7.44.1",
    "@sentry/profiling-node": "^1.3.5",
    "@socket.io/admin-ui": "^0.5.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/lru-cache": "^7.10.9",
    "@types/sanitize-html": "^2.15.0",
    "bcrypt": "^5.1.1",
    "connect-redis": "^8.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.3.0",
    "csurf": "^1.10.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.1.0",
    "mediasoup": "^3.15.7",
    "mediasoup-client": "^3.10.0",
    "module-alias": "^2.2.3",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.2",
    "multer": "^1.4.5-lts.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "sanitize-html": "^2.16.0",
    "sharp": "^0.34.1",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^11.1.0",
    "web-push": "^3.6.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.24.3",
    "zxcvbn": "^4.4.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/chalk": "^2.2.0",
    "@types/express": "^5.0.1",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6.4.17",
    "@types/sharp": "^0.31.1",
    "@types/sinon": "^17.0.3",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.4",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chalk": "^4.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "sinon": "^17.0.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
